<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button ?>
<?import javafx.scene.control.ListView ?>
<?import javafx.scene.layout.Pane ?>

<Pane onKeyPressed="#handleKeyPress" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ListView fx:id="listview" onKeyPressed="#handleListKeyPress" layoutX="200.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="play_btn" onKeyPressed="#handlePlayKeyPress" layoutX="345.0" layoutY="300.0" mnemonicParsing="false" onAction="#handlePlay" prefHeight="27.0" prefWidth="55.0" text="Play" />
   </children>
</Pane>
